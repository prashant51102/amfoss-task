#include <cmath>
#include<string.h>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {   
    string i;
    int zero=0;
    int one=0;
    cin>>i;
    int len=i.length();
    for(int j=0;j<len;j++)
    { if(i[j]=='0')
       ++zero;
        else
    ++one;
     
     }
    if(zero!=one)
        cout<<"2";

    
}
