#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int getMin(int arr[], int n)
{
    int res = arr[0];
    for (int o = 1; o < n; o++)
        res = min(res, arr[o]);
    return res;
}

int main() {
    int variety;
    cin>>variety;
    cout<<"\n";
    int req[variety],noavail[variety];

        for(int i=0;i<variety;i++)
        {cin>>req[i]; }
        cout<<"\n";

        for(int j=0;j<variety;j++)
        {
         cin>>noavail[j];
        }
         cout<<"\n";
        
          for(int k=0;k<variety;k++)
        {
           noavail[k]=noavail[k]/req[k];
           
        } cout<<getMin(noavail,variety);
          



    return 0;
}
